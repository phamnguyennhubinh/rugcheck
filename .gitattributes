# Mark Husky scripts as vendored to exclude from language statistics
.husky/* linguist-vendored=true
.husky/**/* linguist-vendored=true

# Mark generated files as generated to exclude from language statistics
dist/* linguist-generated=true
dist/**/* linguist-generated=true

# Mark other generated JS files
*.js linguist-language=TypeScript
*.d.ts linguist-generated=true
*.d.ts.map linguist-generated=true
*.js.map linguist-generated=true
